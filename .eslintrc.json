{
  "extends": "google",
  "parserOptions": {
    "ecmaVersion": 6
 },

 "env": {
   "es6": true,
   "node": true
 },

 "rules": {
    "require-jsdoc": ["error", {
        "require": {
            "FunctionDeclaration": false,
            "MethodDefinition": false,
            "ClassDeclaration": false,
            "ArrowFunctionExpression": false,
            "FunctionExpression": false
        }
    }],
    "max-len": ["error", {
      "code": 100,
      "ignoreStrings": true,
      "ignoreComments": true
    }],
    "no-tabs": 0
  }

}
